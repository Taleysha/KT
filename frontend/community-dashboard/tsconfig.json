{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "paths": {
        "*": ["node_modules/*"]
      },
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  